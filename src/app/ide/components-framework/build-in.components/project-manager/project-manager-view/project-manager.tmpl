<div class="row project-manager-container">
    <div class="col-lg-3 project-manager-treeview-area">
        <div class="row project-manager-info-actions-area" style="border: 2px solid white; background-color: rgb(74 129 177); padding-top:5px; padding-bottom:5px;">
            <div class="col-10">
                <div class="row">
                    <div class="col-1"></div>
                    <div class="col-1 project-manager-img" style="margin-top: 5px;">
                        <% if(_.includes(img, "fa-")) { %>
                            <i class="fa <%= img %>" aria-hidden="true"></i>
                        <% } else { %>
                            <img src="<%= img %>">
                        <% } %>
                    </div>
                    <div class="col-10 project-manager-title" style="margin-top: 5px;">
                        <%= title %>
                    </div>
                </div>
            </div>
            <div class="col-1 project-manager-actions-area">
                
            </div>
        </div>
        <div class="row">
            <div class="col project-manager-app-instances-view-area" style="margin-top:5px;">
                    No project are loaded yet.
            </div>
        </div>
    </div>
    <div class="col-lg-9" style="position: relative;
    height:90vh;
    border: 1px solid black;
    padding-left: 0px; padding-right:0px;">
        <div class="project-manager-visual-editors-area">
            <div style="text-align: center;">
                <span> Visual Editors Area is not open yet. </span>
            </div>
        </div>
        <div class="project-manager-runtime-console-area"
            style="position: absolute; bottom: 0; width: 100%; height:37vh;">
            <div style="text-align: center;">
                <span> Runtime Console Area is not open yet. </span>
            </div>
        </div>
    </div>
</div>
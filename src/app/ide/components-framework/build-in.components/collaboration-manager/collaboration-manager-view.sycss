/****************************************************************************
---> Settings Popup
****************************************************************************/

.collaboration-all-settings-container{
    min-width: 32vw;
    padding: 2vw;
    display: flex;
    flex-direction: column;
    gap: 20px;
}

.collaboration-settings-category{
    width: 100%;
    text-align: left;
    display: flex;
    flex-direction: column;
    gap: 15px;
}

.collaboration-settings-category-title{
    font-style: normal;
    font-weight: bold;
    font-size: 19px;
    line-height: 22px;

    color: rgba(0, 0, 0, 0.81);
}

.collaboration-setting{
    display: flex;
    align-items: center;
    width: 100%;
    justify-content: space-between;
}

.collaboration-setting-title {
    font-style: normal;
    font-weight: normal;
    font-size: 16px;
    line-height: 18px;

    color: rgba(0, 0, 0, 0.81);
}


.collaboration-settings-apply-button{
    width: 200px;
    height: 35px;
    background: #4A81B1;
    box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 16px;
    color: #FFFFFF;

    display: flex;
    align-items: center;
    justify-content: center;
    align-self: center;
    margin-top: 50px;
    cursor: pointer;
}

.collaboration-setting-checkbox-container {
    position: relative;
    display: inline-block;
    width: 40px;
    height: 14px;
    margin-bottom: 0;
}
  
.collaboration-setting-checkbox-container input { 
    opacity: 0;
    width: 0;
    height: 0;
}
  
.slider {
    position: absolute;
    cursor: pointer;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #C6C5C5;
    -webkit-transition: .4s;
    transition: .4s;
    border-radius: 34px;
}
  
.slider:before {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    content: "";
    height: 20;
    width: 20;

    border-radius: 50%;
    background-color: #F1F1F1;
    -webkit-transition: .4s;
    transition: .4s;
    box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.12), 0px 2px 2px rgba(0, 0, 0, 0.24);
}
  
input:checked + .slider {
    background-color: #80CBC3;
}
  
input:focus + .slider {
    box-shadow: 0 0 1px #2196F3;
}
  
input:checked + .slider:before {
    -webkit-transform: translate(calc(40px - 100%), -50%);
    -ms-transform: translate(calc(40px - 100%), -50%);
    transform: translate(calc(40px - 100%), -50%);
    top: 50%;
    background-color: #009688;
    box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.12), 0px 2px 2px rgba(0, 0, 0, 0.24);
}



.runtime-manager-output {
    flex: 1;
    overflow-y: auto;
    padding: 10px;
}

.runtime-manager-output::-webkit-scrollbar {
    width: 6px;
}

.runtime-manager-output::-webkit-scrollbar-track {
    background: #ddd;
}

.runtime-manager-output::-webkit-scrollbar-thumb {
    background: #bdbdbd;
}

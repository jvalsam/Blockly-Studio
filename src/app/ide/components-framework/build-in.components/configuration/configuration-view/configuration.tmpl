<div class="modal-container modal fade" id="configModalCenter" tabindex="-1" role="dialog" aria-labelledby="configModalTitle" aria-hidden="true">
    <div class="modal-dialog bs-modal-dialog" role="document">
        <div class="modal-content bs-modal-content"  style="background-color:<%= style['Background'] %>;">
            <div class="modal-header">
                <h5 class="modal-title" id="configModalTitle">Configuration of <%= compName %></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid config-properties-body" style="padding: 5px 5px 5px 5px;">
                    
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary ts-btn-config-cancel" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-secondary ts-btn-config-reset">Reset</button>
                <button type="button" class="btn btn-primary ts-btn-config-save">Save</button>
            </div>
    </div>
</div>